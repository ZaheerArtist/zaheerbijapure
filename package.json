{
  "name": "zaheer-bijapure-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "deploy": "next build && next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "deploy:clean": "rm -rf out && npm run deploy",
    "check-links": "next build && next export && node scripts/check-links.js",
    "check-images": "node scripts/check-images.js",
    "validate": "npm run lint && npm run check-images && npm run build"
  },
  "dependencies": {
    "framer-motion": "^10.16.4",
    "next": "14.0.4",
    "nodemailer": "^6.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.5.2",
    "react-particles": "^2.12.2",
    "react-tsparticles": "^2.12.2",
    "tsparticles": "^3.8.1",
    "tsparticles-engine": "^2.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
